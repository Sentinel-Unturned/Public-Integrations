{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022"],
    "outDir": "discord-moderation-webhooks/dist",
    "rootDir": "discord-moderation-webhooks/src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "include": ["discord-moderation-webhooks/src/**/*"],
  "exclude": ["node_modules", "discord-moderation-webhooks/dist"]
}
